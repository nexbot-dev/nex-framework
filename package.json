{
  "name": "@nexbot/nex-framework",
  "version": "0.0.1",
  "description": "A discord bot framework by NexBotDev built on top of discord.js",
  "type": "module",
  "author": "@nexbot-dev",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.5.1",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "imports": {
    "#core/*": [
      "./src/core/*.js"
    ],
    "#libs/*": [
      "./src/libs/*.js"
    ]
  },
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "engines": {
    "node": ">=20.1.0",
    "npm": ">=9.6.7"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.32.4",
    "discord.js": "^14.11.0",
    "terser": "^5.18.2",
    "tsup": "^7.1.0",
    "typescript": "^5.0.4",
    "vitest": "^0.32.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nexbot-dev",
    "nexbot",
    "discord.js",
    "discord",
    "bot",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/nexbot-dev/nexbot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexbot-dev/nexbot.git"
  },
  "bugs": {
    "url": "https://github.com/nexbot-dev/nexbot/issues"
  },
  "files": [
    "dist/"
  ]
}
