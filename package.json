{
  "name": "@nexbot-dev/nex-framework",
  "version": "0.0.1",
  "description": "A discord bot framework by NexBotDev built on top of discord.js",
  "type": "module",
  "repository": "https://github.com/nexbot-dev/nexbot.git",
  "author": "@nexbot-dev",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.5.1",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "node scripts/clean-dist.js"
  },
  "imports": {
    "#core/*": [
      "./src/core/*.js"
    ],
    "#libs/*": [
      "./src/libs/*.js"
    ]
  },
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "engines": {
    "node": ">=20.1.0",
    "npm": ">=9.6.7"
  },
  "devDependencies": {
    "discord.js": "^14.11.0",
    "terser": "^5.18.2",
    "tsup": "^7.1.0",
    "typescript": "^5.0.4"
  }
}
